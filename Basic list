import java.util.Scanner;

class test {
	
	public static void main(String args[])
	{
	int a = 33;
	int b = 44;
	
	a = a+b;
	b = a-b;
	a = a-b;

		System.out.println(a);
		System.out.println(b);
	
    }

}
   
