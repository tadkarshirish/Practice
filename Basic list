Fibonacchi Series program


import java.util.Scanner;

class test {
	
	public static void main(String args[])
	{
		
	int n = 10 , first=0,second=1;
	
	System.out.println("Fibbo series is ");
	
	for(int i=0; i<n;i++)
	{
		System.out.print(first + "," );
		 int next = first + second;
		 first = second;
		 second = next;
		
	}
}

}


