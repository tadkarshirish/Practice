import java.util.Scanner;

class test {
	
	
	public static void main(String args[]){
		
	Scanner s = new Scanner(System.in);
	
	int arr[] = { 1,2,3,4,5,6};
	int l = arr.length-1;

	for(int i=arr.length-1;i>=0;i--)
	{
			System.out.print(arr[i]);
	}	
	
	for( int i = 0 ; i<=arr.length-1;i++,l--)
	{
		int temp = arr[i];
		arr[i]=arr[l];
		arr[l]=temp;
		
	}
	
	System.out.println("");
	
	for(int i=0;i<=arr.length-1;i++)
	{
			System.out.print(arr[i]);
	}
	

}

}

